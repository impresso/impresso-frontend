{
  "name": "impresso-frontend",
  "version": "2.2.0",
  "description": "A Vue.js project",
  "author": "Thijs van Beek <thijs.vanbeek@uni.lu>",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "npm run unit && npm run e2e"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "4.3.3",
    "@feathersjs/feathers": "4.3.3",
    "@feathersjs/socketio-client": "4.3.3",
    "bootstrap-vue": "2.2.2",
    "core-js": "3.6.4",
    "d3": "5.15.0",
    "dripicons": "2.0.0",
    "filepond": "4.9.4",
    "filepond-plugin-file-validate-size": "2.2.0",
    "filepond-plugin-file-validate-type": "1.2.4",
    "flatpickr": "4.6.3",
    "impresso-jscommons": "git+https://git@github.com/impresso/impresso-jscommons.git#master",
    "impresso-theme": "git+https://git@github.com/impresso/impresso-theme.git",
    "markdown-it": "10.0.0",
    "markdown-it-decorate": "1.2.2",
    "openseadragon": "2.4.1",
    "socket.io-client": "2.3.0",
    "svg-overlay": "github:openseadragon/svg-overlay",
    "uuid": "3.4.0",
    "vue": "2.6.10",
    "vue-awesome": "4.0.2",
    "vue-click-outside": "1.0.7",
    "vue-flatpickr-component": "8.1.5",
    "vue-i18n": "8.15.3",
    "vue-router": "3.1.5",
    "vuex": "3.1.2",
    "vuex-persistedstate": "2.7.0",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "0.5.0",
    "@vue/cli-plugin-babel": "4.1.0",
    "@vue/cli-plugin-eslint": "4.1.0",
    "@vue/cli-service": "4.1.0",
    "@vue/eslint-config-airbnb": "5.0.2",
    "babel-eslint": "10.0.3",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-vue": "5.0.0",
    "less": "^3.10.3",
    "less-loader": "5.0.0",
    "node-sass": "4.13.1",
    "postcss-import": "12.0.1",
    "postcss-url": "8.0.0",
    "sass-loader": "8.0.2",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": [
        "warn",
        {
          "allow": [
            "info",
            "warn",
            "error"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never",
          "vue": "never"
        }
      ],
      "import/no-cycle": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
